
# Example environment variables for ICU PPO Backend

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=1

# Model Configuration
MODEL_PATH=models/saved/ppo_icu_best.pkl
TRAINING_DATA_PATH=data/patient_training.csv
BACKUP_MODEL_PATH=models/backup/

# Training Parameters
LEARNING_RATE=0.0003
GAMMA=0.99
PPO_EPSILON=0.2
TRAINING_EPOCHS=1000
BATCH_SIZE=64

# Environment Settings
ICU_CAPACITY=20
WARD_CAPACITY=50
ED_CAPACITY=25

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/ppo_training.log

# Security
SECRET_KEY=your-secret-key-here
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Database (if using external storage)
# DATABASE_URL=postgresql://user:password@localhost/icu_db

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9000
